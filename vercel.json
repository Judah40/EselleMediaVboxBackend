{
  "version": 2,
  "functions": {
    "src/server.js": {
      "runtime": "@vercel/node@3.0.12"
    }
  },
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ],
  "rootDirectory": "src"
}
